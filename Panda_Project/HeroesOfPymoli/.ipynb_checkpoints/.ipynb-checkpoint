    "# PURCHASING ANALYSIS (GENDER): \n",
    "#Run basic calculations to obtain purchase count, avg. purchase price, avg. purchase total per person etc. by gender\n",
    "#Create a summary data frame to hold the results\n",
    "#Optional: give the displayed data cleaner formatting\n",
    "#Display the summary data frame\n",
    "\n",
    "# Calculate PURCHASE COUNT by gender\n",
    "all_gender_pd = purchase_data_pd.groupby('Gender')\n",
    "\n",
    "player_count = all_gender_pd['SN'].count()\n",
    "player_count_pd = pd.DataFrame(player_count)\n",
    "\n",
    "# Calculate AVERAGE PURCHASE PRICE by gender\n",
    "avg_price = all_gender_pd['Price'].mean()\n",
    "# avg_price_pd = pd.DataFrame(avg_price)\n",
    "\n",
    "# Calculate TOTAL PURCHASE PRICE by gender\n",
    "total_gender_purchase = all_gender_pd['Price'].sum()\n",
    "\n",
    "# Calculate Avg Total Purchase Per Person (Total Purchase Value / total count per category)\n",
    "norm_total = total_gender_purchase / all_gender_pd\n",
    "\n",
    "# Add 'Average Purchase Price' to table\n",
    "player_count_pd['Average Purchase Price'] = avg_price\n",
    "gender_purchase_pd = pd.DataFrame(player_count_pd)\n",
    "\n",
    "# Rename 'SN' column to 'Purchase Count'\n",
    "new_gender_purchase_pd = gender_purchase_pd.rename(columns={\n",
    "    'SN': 'Purchase Count',\n",
    "})\n",
    "\n",
    "# Add 'Avg Total Purchase Per Person' to table\n",
    "new_gender_purchase_pd['Total Purchase Value'] = total_gender_purchase\n",
